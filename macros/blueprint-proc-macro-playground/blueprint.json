  "jobs": [
    {
      "metadata": {
        "name": "keygen",
        "description": "Simple Threashold (t) Keygen Job for n parties."
      },
      "params": [
        "Uint16",
        "Uint8"
      ],
      "result": [
        "Bytes"
      ],
      "verifier": {
        "Evm": "contracts/out/KeygenContract.sol/KeygenContract.json"
      }
    },
    {
      "metadata": {
        "name": "sign",
        "description": "Sign a message using a key generated by the keygen job."
      },
      "params": [
        "Uint64",
        "Bytes"
      ],
      "result": [
        "Bytes"
      ],
      "verifier": "None"
    }
  ],
  "registration_hook": {
    "Evm": "contracts/out/RegistrationContract.sol/RegistrationContract.json"
  },
  "registration_params": [
    "Bytes"
  ],
  "request_hook": {
    "Evm": "contracts/out/RequestContract.sol/RequestContract.json"
  },
  "request_params": [
    "Uint64"
  ],
  "gadget": {
    "Wasm": {
      "runtime": "Wasmtime",
      "soruces": []
    }
  }
